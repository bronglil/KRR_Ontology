###  http://tripplanning/ontologies/tripplanning.owl#WeekdaySchedule
:WeekdaySchedule rdf:type owl:Class ;
                 rdfs:subClassOf :Schedule .


###  http://tripplanning/ontologies/tripplanning.owl#WeekendSchedule
:WeekendSchedule rdf:type owl:Class ;
                 rdfs:subClassOf :Schedule .


###  http://www.w3.org/2002/07/owl#Thing
owl:Thing rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :madeBy ;
                            owl:someValuesFrom :Person
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :bookingFor ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ;
          rdfs:comment "This is a platform build for ontologies used in planning trips"@en ;
          rdfs:label "TripPlanning"@en .


