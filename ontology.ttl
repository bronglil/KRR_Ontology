                              [ rdf:type owl:Restriction ;
                                owl:onProperty :usesMode ;
                                owl:allValuesFrom :Train
                              ] .


###  http://tripplanning/ontologies/tripplanning.owl#TrainService
:TrainService rdf:type owl:Class ;
              rdfs:subClassOf :TransportService .


