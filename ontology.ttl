@prefix : <http://tripplanning/ontologies/tripplanning.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://tripplanning/ontologies/tripplanning.owl#> .

<http://tripplanning/ontologies/tripplanning.owl> rdf:type owl:Ontology ;
                                                   rdfs:comment "This is a platform build for ontologies used in planning trips"@en ;
                                                   rdfs:label "This is trip planning ontology"@en .

#################################################################
#    Object Properties
#################################################################

###  http://tripplanning/ontologies/tripplanning.owl#bookingFor
:bookingFor rdf:type owl:ObjectProperty ;
            rdfs:domain :Booking ;
            rdfs:range :TransportService .


###  http://tripplanning/ontologies/tripplanning.owl#connectsWith
:connectsWith rdf:type owl:ObjectProperty ,
                       owl:SymmetricProperty ;
              rdfs:domain :TransportHub ;
              rdfs:range :TransportHub .


###  http://tripplanning/ontologies/tripplanning.owl#followsRoute
:followsRoute rdf:type owl:ObjectProperty ;
              rdfs:domain :TransportService ;
              rdfs:range :Route .


###  http://tripplanning/ontologies/tripplanning.owl#hasAlternative
:hasAlternative rdf:type owl:ObjectProperty ;
                rdfs:domain :TripSegment ;
                rdfs:range :TransportMode .


###  http://tripplanning/ontologies/tripplanning.owl#hasDropoff
:hasDropoff rdf:type owl:ObjectProperty ;
            rdfs:domain :RideshareSegment ;
            rdfs:range :Location .


###  http://tripplanning/ontologies/tripplanning.owl#hasEnd
:hasEnd rdf:type owl:ObjectProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain :Trip ,
                    :TripSegment ;
        rdfs:range :Location .


###  http://tripplanning/ontologies/tripplanning.owl#hasFirstSegment
:hasFirstSegment rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :Itinerary ;
                 rdfs:range :TripSegment .


###  http://tripplanning/ontologies/tripplanning.owl#hasLastSegment
:hasLastSegment rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Itinerary ;
                rdfs:range :TripSegment .


###  http://tripplanning/ontologies/tripplanning.owl#hasPickup
:hasPickup rdf:type owl:ObjectProperty ;
           rdfs:domain :RideshareSegment ;
           rdfs:range :Location .


###  http://tripplanning/ontologies/tripplanning.owl#hasPlan
:hasPlan rdf:type owl:ObjectProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Trip ;
         rdfs:range :Itinerary .


###  http://tripplanning/ontologies/tripplanning.owl#hasSchedule
:hasSchedule rdf:type owl:ObjectProperty ;
             rdfs:domain :Route ,
                         :TransportService ;
             rdfs:range :Schedule .


###  http://tripplanning/ontologies/tripplanning.owl#hasSegment
:hasSegment rdf:type owl:ObjectProperty ;
            rdfs:domain :Itinerary ;
            rdfs:range :TripSegment .


###  http://tripplanning/ontologies/tripplanning.owl#hasStart
:hasStart rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Trip ,
                      :TripSegment ;
          rdfs:range :Location .


###  http://tripplanning/ontologies/tripplanning.owl#hasStop
:hasStop rdf:type owl:ObjectProperty ;
         rdfs:domain :Route ;
         rdfs:range :TransportHub .


###  http://tripplanning/ontologies/tripplanning.owl#locatedIn
:locatedIn rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain :Location ;
           rdfs:range :Location .


###  http://tripplanning/ontologies/tripplanning.owl#madeBy
:madeBy rdf:type owl:ObjectProperty ;
        rdfs:domain :Booking ;
        rdfs:range :Person .


###  http://tripplanning/ontologies/tripplanning.owl#nextSegment
:nextSegment rdf:type owl:ObjectProperty ;
             owl:inverseOf :previousSegment ;
             rdf:type owl:AsymmetricProperty ;
             rdfs:domain :TripSegment ;
             rdfs:range :TripSegment .


###  http://tripplanning/ontologies/tripplanning.owl#operatedBy
:operatedBy rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :TransportService ;
            rdfs:range :Company .


###  http://tripplanning/ontologies/tripplanning.owl#previousSegment
:previousSegment rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ;
                 rdfs:domain :TripSegment ;
                 rdfs:range :TripSegment .


###  http://tripplanning/ontologies/tripplanning.owl#serves
:serves rdf:type owl:ObjectProperty ;
        rdfs:domain :TransportService ;
        rdfs:range :Location .


###  http://tripplanning/ontologies/tripplanning.owl#usesMode
:usesMode rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :TripSegment ;
          rdfs:range :TransportMode .


###  http://tripplanning/ontologies/tripplanning.owl#usesService
:usesService rdf:type owl:ObjectProperty ;
             rdfs:domain :PublicSegment ;
             rdfs:range :TransportService .


#################################################################
#    Data properties
#################################################################

###  http://tripplanning/ontologies/tripplanning.owl#arriveTime
:arriveTime rdf:type owl:DatatypeProperty ;
            rdfs:domain :Trip ,
                        :TripSegment ;
            rdfs:range xsd:dateTime .


###  http://tripplanning/ontologies/tripplanning.owl#city
:city rdf:type owl:DatatypeProperty ;
      rdfs:domain :Location ;
      rdfs:range xsd:string .


###  http://tripplanning/ontologies/tripplanning.owl#code
:code rdf:type owl:DatatypeProperty ;
      rdfs:domain :Booking ,
                  :Route ,
                  :TransportService ;
      rdfs:range xsd:string .


###  http://tripplanning/ontologies/tripplanning.owl#country
:country rdf:type owl:DatatypeProperty ;
         rdfs:domain :Location ;
         rdfs:range xsd:string .


###  http://tripplanning/ontologies/tripplanning.owl#currency
:currency rdf:type owl:DatatypeProperty ;
          rdfs:domain :Booking ,
                      :TripSegment ;
          rdfs:range xsd:string .


###  http://tripplanning/ontologies/tripplanning.owl#departTime
:departTime rdf:type owl:DatatypeProperty ;
            rdfs:domain :Trip ,
                        :TripSegment ;
            rdfs:range xsd:dateTime .


###  http://tripplanning/ontologies/tripplanning.owl#distance
:distance rdf:type owl:DatatypeProperty ;
          rdfs:domain :TripSegment ;
          rdfs:range xsd:decimal .


###  http://tripplanning/ontologies/tripplanning.owl#duration
:duration rdf:type owl:DatatypeProperty ;
          rdfs:domain :Trip ,
                      :TripSegment ;
          rdfs:range xsd:string .


###  http://tripplanning/ontologies/tripplanning.owl#email
:email rdf:type owl:DatatypeProperty ;
       rdfs:domain :Company ,
                   :Person ;
       rdfs:range xsd:string .


###  http://tripplanning/ontologies/tripplanning.owl#flightNumber
:flightNumber rdf:type owl:DatatypeProperty ;
              rdfs:domain :FlightSegment ;
              rdfs:range xsd:string .


###  http://tripplanning/ontologies/tripplanning.owl#gate
:gate rdf:type owl:DatatypeProperty ;
      rdfs:domain :FlightSegment ;
      rdfs:range xsd:string .


###  http://tripplanning/ontologies/tripplanning.owl#latitude
:latitude rdf:type owl:DatatypeProperty ;
          rdfs:domain :Location ;
          rdfs:range xsd:decimal .


###  http://tripplanning/ontologies/tripplanning.owl#longitude
:longitude rdf:type owl:DatatypeProperty ;
           rdfs:domain :Location ;
           rdfs:range xsd:decimal .


###  http://tripplanning/ontologies/tripplanning.owl#maxBaggage
:maxBaggage rdf:type owl:DatatypeProperty ;
            rdfs:domain :TransportService ;
            rdfs:range xsd:integer .


###  http://tripplanning/ontologies/tripplanning.owl#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:domain :Company ,
                  :Location ,
                  :Person ,
                  :Route ,
                  :TransportService ;
      rdfs:range xsd:string .


###  http://tripplanning/ontologies/tripplanning.owl#phoneNumber
:phoneNumber rdf:type owl:DatatypeProperty ;
             rdfs:domain :Company ,
                         :Person ;
             rdfs:range xsd:string .


###  http://tripplanning/ontologies/tripplanning.owl#platformNumber
:platformNumber rdf:type owl:DatatypeProperty ;
                rdfs:domain :TrainSegment ;
                rdfs:range xsd:int .


###  http://tripplanning/ontologies/tripplanning.owl#postalCode
:postalCode rdf:type owl:DatatypeProperty ;
            rdfs:domain :Location ;
            rdfs:range xsd:integer .


###  http://tripplanning/ontologies/tripplanning.owl#price
:price rdf:type owl:DatatypeProperty ;
       rdfs:domain :Booking ,
                   :TripSegment ;
       rdfs:range xsd:decimal .


###  http://tripplanning/ontologies/tripplanning.owl#rating
:rating rdf:type owl:DatatypeProperty ;
        rdfs:domain :RideshareService ,
                    :TransportCompany ;
        rdfs:range xsd:decimal .


###  http://tripplanning/ontologies/tripplanning.owl#status
:status rdf:type owl:DatatypeProperty ;
        rdfs:domain :Booking ;
        rdfs:range xsd:string .


###  http://tripplanning/ontologies/tripplanning.owl#street
:street rdf:type owl:DatatypeProperty ;
        rdfs:domain :Location ;
        rdfs:range xsd:string .


###  http://tripplanning/ontologies/tripplanning.owl#terminal
:terminal rdf:type owl:DatatypeProperty ;
          rdfs:domain :Airport ,
                      :FlightSegment ;
          rdfs:range xsd:string .


###  http://tripplanning/ontologies/tripplanning.owl#website
:website rdf:type owl:DatatypeProperty ;
         rdfs:domain :Company ,
                     :TransportService ;
         rdfs:range xsd:anyURI .


#################################################################
#    Classes
#################################################################

###  http://tripplanning/ontologies/tripplanning.owl#AccommodationBooking
:AccommodationBooking rdf:type owl:Class ;
                      rdfs:subClassOf :Booking ;
                      owl:disjointWith :TransportBooking .


###  http://tripplanning/ontologies/tripplanning.owl#Airline
:Airline rdf:type owl:Class ;
         rdfs:subClassOf :TransportCompany .


###  http://tripplanning/ontologies/tripplanning.owl#AirlineService
:AirlineService rdf:type owl:Class ;
                rdfs:subClassOf :TransportService .


###  http://tripplanning/ontologies/tripplanning.owl#Airport
:Airport rdf:type owl:Class ;
         rdfs:subClassOf :TransportHub .


###  http://tripplanning/ontologies/tripplanning.owl#BikeRentalService
:BikeRentalService rdf:type owl:Class ;
                   rdfs:subClassOf :RentalService ;
                   owl:disjointWith :CarRentalService .


###  http://tripplanning/ontologies/tripplanning.owl#BlaBlaCar
:BlaBlaCar rdf:type owl:Class ;
           rdfs:subClassOf :RideshareService ;
           owl:disjointWith :UberService .


###  http://tripplanning/ontologies/tripplanning.owl#Booking
:Booking rdf:type owl:Class ;
         rdfs:comment "A reservation for a specific transportation service, accommodation, or other travel-related service for a particular date and time."@en ;
         rdfs:label "Booking"@en .


###  http://tripplanning/ontologies/tripplanning.owl#Bus
:Bus rdf:type owl:Class ;
     rdfs:subClassOf :PublicTransport .


###  http://tripplanning/ontologies/tripplanning.owl#BusBooking
:BusBooking rdf:type owl:Class ;
            rdfs:subClassOf :TransportBooking .


###  http://tripplanning/ontologies/tripplanning.owl#BusCompany
:BusCompany rdf:type owl:Class ;
            rdfs:subClassOf :TransportCompany .


###  http://tripplanning/ontologies/tripplanning.owl#BusRoute
:BusRoute rdf:type owl:Class ;
          rdfs:subClassOf :Route .


###  http://tripplanning/ontologies/tripplanning.owl#BusSegment
:BusSegment rdf:type owl:Class ;
            rdfs:subClassOf :PublicSegment ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :usesService ;
                              owl:someValuesFrom :BusService
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :usesMode ;
                              owl:allValuesFrom :Bus
                            ] .


###  http://tripplanning/ontologies/tripplanning.owl#BusService
:BusService rdf:type owl:Class ;
            rdfs:subClassOf :TransportService .


###  http://tripplanning/ontologies/tripplanning.owl#BusStation
:BusStation rdf:type owl:Class ;
            rdfs:subClassOf :TransportHub .


###  http://tripplanning/ontologies/tripplanning.owl#BusinessTrip
:BusinessTrip rdf:type owl:Class ;
              rdfs:subClassOf :Trip .


###  http://tripplanning/ontologies/tripplanning.owl#Car
:Car rdf:type owl:Class ;
     rdfs:subClassOf :PrivateTransport .


###  http://tripplanning/ontologies/tripplanning.owl#CarRentalService
:CarRentalService rdf:type owl:Class ;
                  rdfs:subClassOf :RentalService .


###  http://tripplanning/ontologies/tripplanning.owl#CarSegment
:CarSegment rdf:type owl:Class ;
            rdfs:subClassOf :PrivateSegment .


###  http://tripplanning/ontologies/tripplanning.owl#CheapestItinerary
:CheapestItinerary rdf:type owl:Class ;
                   rdfs:subClassOf :Itinerary .


###  http://tripplanning/ontologies/tripplanning.owl#CityBus
:CityBus rdf:type owl:Class ;
         rdfs:subClassOf :Bus ;
         owl:disjointWith :LongDistanceBus .


###  http://tripplanning/ontologies/tripplanning.owl#CityBusSegment
:CityBusSegment rdf:type owl:Class ;
                rdfs:subClassOf :BusSegment ;
                owl:disjointWith :LongDistanceBusSegment .


###  http://tripplanning/ontologies/tripplanning.owl#CityBusService
:CityBusService rdf:type owl:Class ;
                rdfs:subClassOf :BusService .


###  http://tripplanning/ontologies/tripplanning.owl#CommercialSite
:CommercialSite rdf:type owl:Class ;
                rdfs:subClassOf :Location .


###  http://tripplanning/ontologies/tripplanning.owl#Company
:Company rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :name ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:string
                         ] ;
         rdfs:comment "An organization that provides transportation services, such as a railway company, bus operator, or airline"@en ;
         rdfs:label "Company"@en .


###  http://tripplanning/ontologies/tripplanning.owl#ConnectionSegment
:ConnectionSegment rdf:type owl:Class ;
                   rdfs:subClassOf :TripSegment .


###  http://tripplanning/ontologies/tripplanning.owl#CycleSegment
:CycleSegment rdf:type owl:Class ;
              rdfs:subClassOf :HumanPoweredSegment .


###  http://tripplanning/ontologies/tripplanning.owl#Cycling
:Cycling rdf:type owl:Class ;
         rdfs:subClassOf :HumanPowered .


###  http://tripplanning/ontologies/tripplanning.owl#DirectItinerary
:DirectItinerary rdf:type owl:Class ;
                 rdfs:subClassOf :Itinerary .


###  http://tripplanning/ontologies/tripplanning.owl#DomesticFlightService
:DomesticFlightService rdf:type owl:Class ;
                       rdfs:subClassOf :AirlineService ;
                       owl:disjointWith :InternationalFlightService .


###  http://tripplanning/ontologies/tripplanning.owl#Driver
:Driver rdf:type owl:Class ;
        rdfs:subClassOf :Person .


###  http://tripplanning/ontologies/tripplanning.owl#FastestItinerary
:FastestItinerary rdf:type owl:Class ;
                  rdfs:subClassOf :Itinerary .


###  http://tripplanning/ontologies/tripplanning.owl#Ferry
:Ferry rdf:type owl:Class ;
       rdfs:subClassOf :PublicTransport .


###  http://tripplanning/ontologies/tripplanning.owl#FerryBooking
:FerryBooking rdf:type owl:Class ;
              rdfs:subClassOf :TransportBooking .


###  http://tripplanning/ontologies/tripplanning.owl#FerryCompany
:FerryCompany rdf:type owl:Class ;
              rdfs:subClassOf :TransportCompany .


###  http://tripplanning/ontologies/tripplanning.owl#FerryRoute
:FerryRoute rdf:type owl:Class ;
            rdfs:subClassOf :Route .


###  http://tripplanning/ontologies/tripplanning.owl#FerrySegment
:FerrySegment rdf:type owl:Class ;
              rdfs:subClassOf :PublicSegment .


###  http://tripplanning/ontologies/tripplanning.owl#FerryService
:FerryService rdf:type owl:Class ;
              rdfs:subClassOf :TransportService .


###  http://tripplanning/ontologies/tripplanning.owl#Flight
:Flight rdf:type owl:Class ;
        rdfs:subClassOf :PublicTransport .


###  http://tripplanning/ontologies/tripplanning.owl#FlightBooking
:FlightBooking rdf:type owl:Class ;
               rdfs:subClassOf :TransportBooking .


###  http://tripplanning/ontologies/tripplanning.owl#FlightRoute
:FlightRoute rdf:type owl:Class ;
             rdfs:subClassOf :Route .


###  http://tripplanning/ontologies/tripplanning.owl#FlightSegment
:FlightSegment rdf:type owl:Class ;
               rdfs:subClassOf :PublicSegment ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :usesService ;
                                 owl:someValuesFrom :AirlineService
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :usesMode ;
                                 owl:allValuesFrom :Flight
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :flightNumber ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:string
                               ] .


###  http://tripplanning/ontologies/tripplanning.owl#FlixbusService
:FlixbusService rdf:type owl:Class ;
                rdfs:subClassOf :BusService .


###  http://tripplanning/ontologies/tripplanning.owl#GolfCourse
:GolfCourse rdf:type owl:Class ;
            rdfs:subClassOf :RecreationSite ;
            owl:disjointWith :Park .


###  http://tripplanning/ontologies/tripplanning.owl#HighSpeedTrain
:HighSpeedTrain rdf:type owl:Class ;
                rdfs:subClassOf :Train .


###  http://tripplanning/ontologies/tripplanning.owl#HighSpeedTrainService
:HighSpeedTrainService rdf:type owl:Class ;
                       rdfs:subClassOf :TrainService .


###  http://tripplanning/ontologies/tripplanning.owl#HolidaySchedule
:HolidaySchedule rdf:type owl:Class ;
                 rdfs:subClassOf :Schedule .


###  http://tripplanning/ontologies/tripplanning.owl#HomeLocation
:HomeLocation rdf:type owl:Class ;
              rdfs:subClassOf :Location .


###  http://tripplanning/ontologies/tripplanning.owl#HospitalityCompany
:HospitalityCompany rdf:type owl:Class ;
                    rdfs:subClassOf :Company ;
                    owl:disjointWith :TransportCompany .


###  http://tripplanning/ontologies/tripplanning.owl#Hotel
:Hotel rdf:type owl:Class ;
       rdfs:subClassOf :CommercialSite ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :bookingFor ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :Hotel
                       ] ;
       owl:disjointWith :Restaurant .


###  http://tripplanning/ontologies/tripplanning.owl#HotelBooking
:HotelBooking rdf:type owl:Class ;
              rdfs:subClassOf :AccommodationBooking .


###  http://tripplanning/ontologies/tripplanning.owl#HotelChain
:HotelChain rdf:type owl:Class ;
            rdfs:subClassOf :HospitalityCompany .


###  http://tripplanning/ontologies/tripplanning.owl#HumanPowered
:HumanPowered rdf:type owl:Class ;
              rdfs:subClassOf :TransportMode .


###  http://tripplanning/ontologies/tripplanning.owl#HumanPoweredSegment
:HumanPoweredSegment rdf:type owl:Class ;
                     rdfs:subClassOf :TripSegment .


###  http://tripplanning/ontologies/tripplanning.owl#InterCityBusService
:InterCityBusService rdf:type owl:Class ;
                     rdfs:subClassOf :BusService .


###  http://tripplanning/ontologies/tripplanning.owl#IntercityTrain
:IntercityTrain rdf:type owl:Class ;
                rdfs:subClassOf :Train .


###  http://tripplanning/ontologies/tripplanning.owl#IntercityTrainService
:IntercityTrainService rdf:type owl:Class ;
                       rdfs:subClassOf :TrainService .


###  http://tripplanning/ontologies/tripplanning.owl#InternationalFlightService
:InternationalFlightService rdf:type owl:Class ;
                            rdfs:subClassOf :AirlineService .


###  http://tripplanning/ontologies/tripplanning.owl#Itinerary
:Itinerary rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :hasSegment ;
                             owl:someValuesFrom :TripSegment
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasSegment ;
                             owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                             owl:onClass :TripSegment
                           ] ;
           rdfs:comment "A complete plan of a trip, consisting of ordered trip segments that together form the journey from the origin to the destination."@en ;
           rdfs:label "Itinerary"@en .


###  http://tripplanning/ontologies/tripplanning.owl#LeisureTrip
:LeisureTrip rdf:type owl:Class ;
             rdfs:subClassOf :Trip .


###  http://tripplanning/ontologies/tripplanning.owl#Location
:Location rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :latitude ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange [ rdf:type rdfs:Datatype ;
                                              owl:onDatatype xsd:decimal ;
                                              owl:withRestrictions ( [ xsd:minInclusive -90.0
                                                                     ]
                                                                     [ xsd:maxInclusive 90.0
                                                                     ]
                                                                   )
                                            ]
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :longitude ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange [ rdf:type rdfs:Datatype ;
                                              owl:onDatatype xsd:decimal ;
                                              owl:withRestrictions ( [ xsd:minInclusive -180.0
                                                                     ]
                                                                     [ xsd:maxInclusive 180.0
                                                                     ]
                                                                   )
                                            ]
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :name ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] ;
          rdfs:comment "A  area relevant to trip planning, such as a departure area, destination, or intermediate stop. Locations can be residential addresses, transportation hubs, or destination."@en ;
          rdfs:label "Location"@en .


###  http://tripplanning/ontologies/tripplanning.owl#LongDistanceBus
:LongDistanceBus rdf:type owl:Class ;
                 rdfs:subClassOf :Bus .


###  http://tripplanning/ontologies/tripplanning.owl#LongDistanceBusSegment
:LongDistanceBusSegment rdf:type owl:Class ;
                        rdfs:subClassOf :BusSegment .


###  http://tripplanning/ontologies/tripplanning.owl#Metro
:Metro rdf:type owl:Class ;
       rdfs:subClassOf :PublicTransport .


###  http://tripplanning/ontologies/tripplanning.owl#MetroRoute
:MetroRoute rdf:type owl:Class ;
            rdfs:subClassOf :Route .


###  http://tripplanning/ontologies/tripplanning.owl#MetroSegment
:MetroSegment rdf:type owl:Class ;
              rdfs:subClassOf :PublicSegment .


###  http://tripplanning/ontologies/tripplanning.owl#MetroService
:MetroService rdf:type owl:Class ;
              rdfs:subClassOf :TransportService .


###  http://tripplanning/ontologies/tripplanning.owl#MetroStation
:MetroStation rdf:type owl:Class ;
              rdfs:subClassOf :TransportHub .


###  http://tripplanning/ontologies/tripplanning.owl#MultiModalItinerary
:MultiModalItinerary rdf:type owl:Class ;
                     rdfs:subClassOf :Itinerary .


###  http://tripplanning/ontologies/tripplanning.owl#OneWayTrip
:OneWayTrip rdf:type owl:Class ;
            rdfs:subClassOf :Trip .


###  http://tripplanning/ontologies/tripplanning.owl#Park
:Park rdf:type owl:Class ;
      rdfs:subClassOf :RecreationSite .


###  http://tripplanning/ontologies/tripplanning.owl#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :name ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ;
        rdfs:comment "An individual who travels or uses transportation services. A person can be associated with bookings and trips"@en ;
        rdfs:label "Person"@en .


