###  http://tripplanning/ontologies/tripplanning.owl#TransportBooking
:TransportBooking rdf:type owl:Class ;
                  rdfs:subClassOf :Booking ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :bookingFor ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :TransportService
                                  ] .


###  http://tripplanning/ontologies/tripplanning.owl#TransportCompany
:TransportCompany rdf:type owl:Class ;
                  rdfs:subClassOf :Company ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :operatedBy ;
                                    owl:someValuesFrom :TransportService
                                  ] .


###  http://tripplanning/ontologies/tripplanning.owl#TransportHub
:TransportHub rdf:type owl:Class ;
              rdfs:subClassOf :Location ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :connectsWith ;
                                owl:someValuesFrom :TransportHub
                              ] .


###  http://tripplanning/ontologies/tripplanning.owl#TransportMode
:TransportMode rdf:type owl:Class ;
               rdfs:comment "The method of transportation used for a trip segment, such as train, bus, car, or walking"@en ;
               rdfs:label "Transportation Mode"@en .


###  http://tripplanning/ontologies/tripplanning.owl#TransportService
:TransportService rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty :serves ;
                                    owl:someValuesFrom :Location
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :operatedBy ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :Company
                                  ] ;
                  rdfs:comment "A specific service provided by a transportation company, such as a train line, bus route, or flight route"@en ;
                  rdfs:label "Transportation Service"@en .


###  http://tripplanning/ontologies/tripplanning.owl#Traveler
:Traveler rdf:type owl:Class ;
          rdfs:subClassOf :Person ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :madeBy ;
                            owl:someValuesFrom :Booking
                          ] .


###  http://tripplanning/ontologies/tripplanning.owl#Trip
:Trip rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :hasEnd ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Location
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasPlan ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Itinerary
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasStart ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Location
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :arriveTime ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:dateTime
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :departTime ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:dateTime
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :duration ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:string
                      ] ;
      rdfs:comment "A journey from one location to another, consisting of one or more segments using various modes of transportation. A trip has a defined start point, end point, and follows an itinerary."@en ;
      rdfs:label "Trip"@en .


###  http://tripplanning/ontologies/tripplanning.owl#TripSegment
:TripSegment rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :hasEnd ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :Location
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasStart ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :Location
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :usesMode ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :TransportMode
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :arriveTime ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:dateTime
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :departTime ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:dateTime
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :distance ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:decimal
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :duration ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ;
             rdfs:comment "A single portion of a trip using one specific mode of transportation between two locations. Multiple trip segments form a complete itinerary"@en ;
             rdfs:label "Trip Segment"@en .


###  http://tripplanning/ontologies/tripplanning.owl#UberService
:UberService rdf:type owl:Class ;
